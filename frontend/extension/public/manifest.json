{
	"name": "TextData",
	"description": "Save what you know and find what you don't.",
	"version": "0.0.0.6",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "Open TextData"
	},
	"icons": {
		"16": "tree16.png",
		"48": "tree48.png",
		"128": "tree128.png"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"scripting"
	]
}